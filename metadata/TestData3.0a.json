	"data": {
		"command": "\"$NODEEXE\" \"$HAPISERVERPATH/bin/TestData3.0a.js\" --id ${id} --parameters ${parameters} --start ${start} --stop ${stop}",
		"formats": ["csv"],
		"contact": "rweigel@gmu.edu"
	},
  "about": {
        "id": "TestData3.0",
        "title": "Server using HAPI 3.0 additions",
				"HAPI": "3.0",
        "contact": "rweigel@gmu.edu"
  },
	"catalog_inline" :
		[
			{
				"id": "dataset1",
				"title": "Dataset with HAPI 3.0 additions",
				"info": 
						{
							"startDate": "1970-01-01Z",
							"stopDate" : "2016-12-31Z",
							"sampleStartDate": "1970-01-01Z",
							"sampleStopDate" : "1970-01-01T00:01:11Z",
							"cadence": "PT1S",
							"unitsSchema": "udunits2",
							"definitions": {
								"fill": "1e-31",
								"frequency_centers": [ 1,    3,    5],
								"frequency_ranges": [[0,2],[2,4],[4,6]],
								"pitch_angle_centers": [ 11.25,      45,      79.75],
								"pitch_angle_ranges": [[0,22.5],[22.5,67.5],[67.5,90]]
							},
							"parameters": [
                {
                  "name": "Time",
                  "type": "isotime",
                  "units": "UTC",
                  "fill": null,
                  "length": 24
                },
                {
                  "name": "spectra_time_dependent_bins",
                  "type": "double",
                  "units": "m",
                  "fill": "-1e31",
                  "size" : [3],
                  "description": "A 3-element spectra with time-dependent bins",
                  "bins": [
                            {
                              "name": "frequency",
                              "units": "Hz",
                              "centers": "frequency_centers",
                              "ranges": "frequency_ranges"
                            }
                      ]
                },
                {
                  "name": "frequency_centers",
                  "type": "double",
                  "units": "Hz",
                  "fill": null,
                  "size": [3],
                  "description": "Bin centers for spectra frequencies"
                },
                {
                  "name": "frequency_ranges",
                  "type": "double",
                  "units": "Hz",
                  "fill": null,
                  "size": [2,3],
                  "description": "Bin ranges for spectra frequencies"
                }
              ]
						}
			}
		]
}

